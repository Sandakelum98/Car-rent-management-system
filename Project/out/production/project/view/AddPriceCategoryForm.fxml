         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnCancel" layoutX="201.0" layoutY="302.0" onAction="#cancelBtnClickAction" onMouseEntered="#cancelBtnEnter" onMouseExited="#cancelBtnExit" prefHeight="25.0" prefWidth="73.0" style="-fx-background-color: #2d98da;" text="Cancel" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
