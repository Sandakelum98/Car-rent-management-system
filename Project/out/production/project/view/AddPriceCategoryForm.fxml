<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="352.0" prefWidth="386.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddPriceCategoryFormController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Home   &gt;   Add Price Category" />
      <Label layoutX="14.0" layoutY="31.0" text="Add Price Category" textFill="#2d98da">
         <font>
            <Font name="Ebrima" size="24.0" />
         </font>
      </Label>
      <JFXTextField fx:id="txtCategoryName" layoutX="183.0" layoutY="110.0" onAction="#categoryNameTxtAction" />
      <Label layoutX="68.0" layoutY="113.0" text="Category Name  :">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <JFXTextField fx:id="txtDefaultRate" layoutX="183.0" layoutY="151.0" onAction="#defaultRateTxtAction" />
      <Label layoutX="68.0" layoutY="154.0" text="Default Rate  :">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <JFXTextField fx:id="txtKm" layoutX="183.0" layoutY="191.0" onAction="#kmTxtAction" />
      <Label layoutX="68.0" layoutY="194.0" text="Km  :">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <JFXTextField fx:id="txtAddingRate" layoutX="183.0" layoutY="231.0" onAction="#addingRateTxtAction" />
